{
  "name": "yodaspeak-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node ./src/bin/www",
    "lint": "eslint --fix",
    "format": "prettier --write ."
  },
  "nodemonConfig": {
    "ext": "*.js,*.nunjucks,*.scss",
    "events": {
      "restart": "npm run lint && npm run format"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0",
    "nodemon": "^2.0.4",
    "nunjucks": "^3.2.2"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "prettier": "2.0.5"
  }
}
